   "source": [
    "import tensorflow as tf\n",
    "#输入张量\n",
    "x=tf.constant([[1,2,1] ,[2,2,2]] ,tf.float32)\n",
    "# 分别对每一行(沿1方向)进行softmax处理\n",
    "#softmax处理\n",
    "s=tf.nn.softmax(x, 1)\n",
    "# 创建会话\n",
    "session = tf.Session()\n",
    "#打印结果\n",
    "print(session.run(s))"
   ]
  }
