   "source": [
    "import tensorflow as tf\n",
    "#输入张量\n",
    "t=tf.constant([2,5,3] ,tf.float32)\n",
    "x=tf.log(t)\n",
    "#softmax处理\n",
    "s=tf.nn.softmax(x, 0)\n",
    "# 创建会话\n",
    "session = tf.Session()\n",
    "#打印结果\n",
    "print(session.run(s))"
   ]
  }
