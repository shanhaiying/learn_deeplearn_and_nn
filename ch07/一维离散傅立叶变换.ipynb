   "source": [
    "import tensorflow as tf\n",
    "#输入长度为3的一维张量\n",
    "f=tf.constant([4,5,6] , tf.complex64)\n",
    "#创建会话\n",
    "session=tf.Session()\n",
    "#一维傅立叶变换\n",
    "F=tf.fft(f)\n",
    "#打印傅立叶变换F的值\n",
    "print(\"傅立叶变换F的值:\")\n",
    "print(session.run(F))\n",
    "#计算F的傅立叶逆变换(显然与输入的f是相等的)\n",
    "F_ifft=tf.ifft(F)\n",
    "print(\"打印F的傅立叶逆变换的值(显然与输入的f是相等的):\")\n",
    "print(session.run(F_ifft))"
   ]
  }
 ],
